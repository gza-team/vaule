delta.Settings:AddInput("SpeedHacks", "input your speed like.", function(sp)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = sp
end)

delta:SetCurrentSuggestionScript("Power", "GZA TEAM", function()
  print("Clicked!")
end)

delta:AddTab("gza-team.exe", "Executor On PCðŸ˜ˆ")

delta:SaveScript("Blade Ball V2", "loadstring(game:HttpGet("https://rawscripts.net/raw/Blade-Ball-GZA-V2-7858"))()", "GZA TEAM")

delta.Settings:AddSwitch("AntiLag", "Antilag.", false, function(state)
  if state then
    	local Terrain = workspace:FindFirstChildOfClass('Terrain')
	Terrain.WaterWaveSize = 0
	Terrain.WaterWaveSpeed = 0
	Terrain.WaterReflectance = 0
	Terrain.WaterTransparency = 0
	Lighting.GlobalShadows = false
	Lighting.FogEnd = 9e9
	settings().Rendering.QualityLevel = 1
	for i,v in pairs(game:GetDescendants()) do
		if v:IsA("Part") or v:IsA("UnionOperation") or v:IsA("MeshPart") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
		elseif v:IsA("Decal") then
			v.Transparency = 1
		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
			v.Lifetime = NumberRange.new(0)
		elseif v:IsA("Explosion") then
			v.BlastPressure = 1
			v.BlastRadius = 1
		end
	end
	for i,v in pairs(Lighting:GetDescendants()) do
		if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("DepthOfFieldEffect") then
			v.Enabled = false
		end
	end
	workspace.DescendantAdded:Connect(function(child)
		task.spawn(function()
			if child:IsA('ForceField') then
				RunService.Heartbeat:Wait()
				child:Destroy()
			elseif child:IsA('Sparkles') then
				RunService.Heartbeat:Wait()
				child:Destroy()
			elseif child:IsA('Smoke') or child:IsA('Fire') then
				RunService.Heartbeat:Wait()
				child:Destroy()
			end
		end)
	end)
  else
    print("Disabled")
  end
end)
end)

local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("GZA TOOL", "14860587530")

local tab = DrRayLibrary.newTab("Plugin", "14860587530")

tab.newButton("Click Update Plugin", "Update New in 24/9/2023", function()
    local key = game:HttpGet("https://raw.githubusercontent.com/gza-team/vaule/main/Key")
writefile("DeltaPlugins/plugingza.txt", key)
end)

tab.newButton("Keyless Delta", "Remove Key", function()
game.CoreGui.Delta.KeySystem.Holder.Visible = false
game.CoreGui.Delta.Executor.Visible = true 
game.CoreGui.Delta.Sidebar.Visible = true
game.CoreGui.Delta.Sidebar.Position = UDim2.new(1, 0, 0.474000007, 0)
    local key2 = game:HttpGet("https://raw.githubusercontent.com/gza-team/vaule/main/keyless")
writefile("DeltaPlugins/plugingzakeyless.txt", key2)
end)

tab.newButton("Open Tab/Menu", "Tab/Menu", function() 
game.CoreGui.Delta.Sidebar.Position = UDim2.new(1, 0, 0.474000007, 0)
end)
